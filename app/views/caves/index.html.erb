<div class="jumbotron">
  <h1>CoWorking Space That Welcomes You</h1>
  <h2>Find the best places to meet, work, create, and make</h2>
</div>


<%- model_class = Cafe -%>
<div class="page-header">
  <h1>Cafes</h1>
</div>

<div class="row">
  <% @caves.each do |cafe| %>
    <div class="col-md-3">
      <div class="thumbnail">
        <%= link_to image_tag(cafe.image.url), cafe %>
        <div class="caption">
          <h3><%= cafe.name %></h3>
          <p>
            <%= cafe.description %><br><br>
            <%= cafe.address %><br>
            <%= cafe.city %>
            <%= cafe.state %>
            <%= cafe.zipcode %><br>
          </p><br>

        </div>
        </div>
      </div>
      <%end %>
    </div>
